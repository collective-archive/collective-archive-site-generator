<div class='row'>
  Material: {{ material }}
</div>

<div class='row'>
  Measurements: {{ measurements }}
</div>

<div class='row'>
  Location: {{ relationships.location }}
</div>

<div class='row'>
  Work Type: {{ workType.workType }}
</div>

<div class='row'>
  Work Type Notes: {{ workType.workTypeNotes }}
</div>

<div class='row'>
  Culture: {{ culture.culture }}
</div>

<div class='row'>
  Culture Notes: {{ culture.notes }}
</div>

